{
  "parser": "typescript",
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxBracketSameLine": true,
  "semi": false,
  "endOfLine": "lf",
  "overrides": [
    { "files": "*.json", "options": { "parser": "json" } },
    { "files": "*.css", "options": { "parser": "css" } },
    { "files": "*.md", "options": { "parser": "markdown" } },
    { "files": "*.mdx", "options": { "parser": "mdx" } }
  ]
}
